@model IEnumerable<Employee>

<h2>Matched Employees</h2>
<table class="table">
    <thead>
        <tr>
            <th>Number</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Gender</th>
            <th>Date of Birth</th>
            <th>Number of Employees</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Number</td>
                <td>@employee.FirstName</td>
                <td>@employee.LastName</td>
                <td>@employee.Gender</td>
                <td>@employee.DateOfBirth.ToShortDateString()</td>
                <td>@employee.NumberOfEmployees</td>
            </tr>
        }
    </tbody>
    <a href="@Url.Action("DownloadFile", "Employee", new { fileName = ViewData["fileName"] })" class="btn btn-primary">
        <i class="bi bi-save"></i> 
        Download File
    </a>
</table>

<script>

</script>